   "outputs": [],
   "source": [
    "def sat(x1, x2):\n",
    "    return (x1 or not x2) and (not x1 and x2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "False False ---> False\n",
      "False True ---> False\n",
      "True False ---> False\n",
      "True True ---> False\n"
     ]
    }
   ],
   "source": [
    "for (x1, x2) in list(itertools.product([False, True], repeat=2)):\n",
    "    print(x1, x2, '--->', sat(x1, x2))"
   ]
  }
